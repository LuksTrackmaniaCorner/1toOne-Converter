                    <Label Content="Testmap2" HorizontalAlignment="Left" Margin="18.33,-0.165,0,0" VerticalAlignment="Top" Width="156.462" Height="31.177" FontSize="13" FontWeight="Bold"/>
                    <Label Content="by florenzius" HorizontalAlignment="Left" Margin="18.33,15.175,0,-2.836" VerticalAlignment="Top" Width="143.129" Height="28.671" FontSize="13"/>
                    <Rectangle HorizontalAlignment="Left" Margin="0,0.502,0,0" Width="8.771" Fill="#FFFF0202" Height="40.011" VerticalAlignment="Top"/>
                    <Label Content="Map file not valid" HorizontalAlignment="Left" Margin="253.992,7.999,0,0" VerticalAlignment="Top" Opacity="0.5"/>
                </Grid>
                <Grid x:Name="Entry1_Copy2" Height="41.01" Width="367.437">
                    <Rectangle HorizontalAlignment="Left" Margin="0,0.502,0,0" Width="367.437" Fill="{DynamicResource SelectedBackgroundBrush}" Height="40.011" VerticalAlignment="Top"/>
                    <Image HorizontalAlignment="Left" Height="40.011" Margin="8.75,0.502,0,0" VerticalAlignment="Top" Width="136.499" Source="image/banner/BayBanner.png" Opacity="0.235"/>
                    <Label Content="TMS A01" HorizontalAlignment="Left" Margin="18.33,-0.165,0,0" VerticalAlignment="Top" Width="156.462" Height="31.177" FontSize="13" FontWeight="Bold"/>
                    <Label Content="by Nadeo" HorizontalAlignment="Left" Margin="18.33,15.175,0,-2.836" VerticalAlignment="Top" Width="143.129" Height="28.671" FontSize="13"/>
                    <Rectangle HorizontalAlignment="Left" Margin="0,0.502,0,0" Width="8.771" Fill="#FF00BAFF" Height="40.011" VerticalAlignment="Top"/>
                    <Image HorizontalAlignment="Left" Height="17.18" Margin="339.771,12.168,0,0" VerticalAlignment="Top" Width="16.333" Source="image/Checkmark.png" Opacity="0.25"/>
                </Grid>
                <Grid x:Name="Entry1_Copy3" Height="41.01" Width="367.437">
                    <Rectangle HorizontalAlignment="Left" Margin="0,0.502,0,0" Width="367.437" Fill="{DynamicResource SelectedBackgroundBrush}" Height="40.011" VerticalAlignment="Top"/>
                    <Image HorizontalAlignment="Left" Height="40.011" Margin="8.75,0.502,0,0" VerticalAlignment="Top" Width="136.499" Source="image/banner/BayBanner.png" Opacity="0.235"/>
                    <Label Content="TMS A02" HorizontalAlignment="Left" Margin="18.33,-0.165,0,0" VerticalAlignment="Top" Width="156.462" Height="31.177" FontSize="13" FontWeight="Bold"/>
                    <Label Content="by Nadeo" HorizontalAlignment="Left" Margin="18.33,15.175,0,-2.836" VerticalAlignment="Top" Width="143.129" Height="28.671" FontSize="13"/>
                    <Rectangle HorizontalAlignment="Left" Margin="0,0.502,0,0" Width="8.771" Fill="#FF00BAFF" Height="40.011" VerticalAlignment="Top"/>
                    <Image HorizontalAlignment="Left" Height="17.18" Margin="339.771,12.168,0,0" VerticalAlignment="Top" Width="16.333" Source="image/Checkmark.png" Opacity="0.25"/>
                </Grid>
                <Grid x:Name="Entry1_Copy4" Height="41.01" Width="367.437">
                    <Rectangle HorizontalAlignment="Left" Margin="0,0.502,0,0" Width="367.437" Fill="{DynamicResource SelectedBackgroundBrush}" Height="40.011" VerticalAlignment="Top"/>
                    <Image HorizontalAlignment="Left" Height="40.011" Margin="8.75,0.502,0,0" VerticalAlignment="Top" Width="136.499" Source="image/banner/BayBanner.png" Opacity="0.235"/>
                    <Label Content="TMS A03" HorizontalAlignment="Left" Margin="18.33,-0.165,0,0" VerticalAlignment="Top" Width="156.462" Height="31.177" FontSize="13" FontWeight="Bold"/>
                    <Label Content="by Nadeo" HorizontalAlignment="Left" Margin="18.33,15.175,0,-2.836" VerticalAlignment="Top" Width="143.129" Height="28.671" FontSize="13"/>
                    <Rectangle HorizontalAlignment="Left" Margin="0,0.502,0,0" Width="8.771" Fill="#FF00BAFF" Height="40.011" VerticalAlignment="Top"/>
                    <Image HorizontalAlignment="Left" Height="17.18" Margin="339.771,12.168,0,0" VerticalAlignment="Top" Width="16.333" Source="image/Checkmark.png" Opacity="0.25"/>
                </Grid>
                <Grid x:Name="Entry1_Copy5" Height="41.01" Width="367.437">
                    <Rectangle HorizontalAlignment="Left" Margin="0,0.502,0,0" Width="367.437" Fill="{DynamicResource SelectedBackgroundBrush}" Height="40.011" VerticalAlignment="Top"/>
                    <Image HorizontalAlignment="Left" Height="40.011" Margin="8.75,0.502,0,0" VerticalAlignment="Top" Width="136.499" Source="image/banner/BayBanner.png" Opacity="0.235"/>
                    <Label Content="TMS A03" HorizontalAlignment="Left" Margin="18.33,-0.165,0,0" VerticalAlignment="Top" Width="156.462" Height="31.177" FontSize="13" FontWeight="Bold"/>
                    <Label Content="by Nadeo" HorizontalAlignment="Left" Margin="18.33,15.175,0,-2.836" VerticalAlignment="Top" Width="143.129" Height="28.671" FontSize="13"/>
                    <Rectangle HorizontalAlignment="Left" Margin="0,0.502,0,0" Width="8.771" Fill="#FF00BAFF" Height="40.011" VerticalAlignment="Top"/>
                    <Image HorizontalAlignment="Left" Height="17.18" Margin="339.771,12.168,0,0" VerticalAlignment="Top" Width="16.333" Source="image/Checkmark.png" Opacity="0.25"/>
                </Grid>
            </ListBox>

            <Image Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Height="150" Width="150" Margin="5,0,0,0" VerticalAlignment="Center" Source="image/Extremist«.Challenge.bmp" />
            <Image Grid.Row="2" Grid.Column="1" HorizontalAlignment="Center" Height="25" VerticalAlignment="Center" Width="25" Source="image/Bay.png" />
            <Label Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="2" Content="Extremist«" VerticalAlignment="Center"  FontSize="14" Height="25" />
            <Separator Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="3" Margin="5,2,0,2"/>
            <Label Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="3" Content="Author: jenox" VerticalAlignment="Center" FontSize="13" Height="25" />
            <Label Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="3" Content="Time: 0:29.330" VerticalAlignment="Center" FontSize="13" Height="25" />
            <Label Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="2" Content="Mood:" VerticalAlignment="Top" FontSize="13" Height="25" />
            <ComboBox Grid.Row="6" Grid.Column="3" VerticalAlignment="Center" FontSize="13" SelectedIndex="1" Height="25">
                <ComboBoxItem Content="Sunrise" FontSize="12"/>
                <ComboBoxItem Content="Day" FontSize="12" FontWeight="Bold"/>
                <ComboBoxItem Content="Sunset" FontSize="12"/>
                <ComboBoxItem Content="Night" FontSize="12"/>
            </ComboBox>
            <Label Grid.Row="7" Grid.Column="1" Grid.ColumnSpan="2" Content="Car:" VerticalAlignment="Top" Height="25" FontSize="13"/>
            <ComboBox Grid.Row="7" Grid.Column="3" VerticalAlignment="Center" FontSize="13" SelectedIndex="1" Height="25">
                <ComboBoxItem Content="SnowCar" FontSize="12"/>
                <ComboBoxItem Content="BayCar" FontSize="12" FontWeight="Bold"/>
                <ComboBoxItem Content="DesertCar" FontSize="12"/>
            </ComboBox>
        </Grid>
    </DockPanel>
</Window>
